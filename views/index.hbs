          
<section class="container-fluid mt-5 p-0 shadow" id="destinos">

<div id="carousel">
     <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
  <ol class="carousel-indicators">
    <li data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"></li>
    <li data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"></li>
    <li data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="../img/carousel1.jpg" class="d-block w-80" alt="...">
      <div class="carousel-caption d-none d-md-block">
        
      </div>
    </div>
    <div class="carousel-item">
      <img src="../img/carousel2.jpg" class="d-block w-80" alt="...">
      <div class="carousel-caption d-none d-md-block">
       
      </div>
    </div>
    <div class="carousel-item">
      <img src="../img/carousel3.jpg" class="d-block w-80" alt="...">
      <div class="carousel-caption d-none d-md-block">
       
      </div>
    </div>
  </div>

</div>

</div>
    <div class="m-0 row mt-5 justify-content-center" id="consul">

        {{#each consulta}}

        <div class="col-md-3 col-sm-6 row justify-content-center" >
            <div class="card  my-5 col-10" style="width: 18rem;">
                <img src="/img/{{imagen}}" class="card-img-top mt-1" alt="Imagen">
                    <div class="card-body">
                        <h5 class="card-title">{{destino}}</h5>
                        <p class="card-text">{{{especificaciones}}}</p>
                        <a href="/{{id}}" class="btn btn-primary">usd{{precio}}</a>
                    </div>
            </div>
        </div>

        {{/each}}

    </div>

</section>
<section class="container-fluid py-5" id="contacto">
    
    <div class="container">

        <form class="px-5" method="POST">
        <div class="form-group row my-4">
            <label for="email" class="col-sm-2 col-form-label">Email</label>
            <div class="col-sm-8">
            <input type="email" class="form-control" required id="email" name="email" value="{{email}}">
            </div>
        </div>
        <div class="form-group row my-4">
            <label for="nombre" class="col-sm-2 col-form-label">Nombre y apellido</label>
            <div class="col-sm-8">
            <input type="text" class="form-control" required id="nombre" name="nombre" value="{{nombre}}">
            </div>
        </div>
        <div class="form-group row my-4">
            <label for="telefono" class="col-sm-2 col-form-label">Telefono</label>
            <div class="col-sm-8">
            <input type="text" class="form-control" required id="telefono" name="telefono" value="{{telefono}}">
            </div>
        </div>
         <div class="form-group row my-4">
            <label for="consulta" class="col-sm-2 col-form-label">Consulta</label>
            <div class="col-sm-8">

                 <textarea class="form-control" required id="consulta"name="consulta" rows="3"></textarea>
            </div>
           
        </div>
        <div class="form-group row">
            <div class="col-sm-2"></div>
            <div class="col-sm-10">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="gridCheck1" name="novedades">
                    <label class="form-check-label" for="gridCheck1">
                    Recibir novedades
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group row justify-content-center mt-5">
            <div class="col-sm-1">
            <button type="submit" class="btn btn-primary">Enviar</button>
            </div>
        </div>
        </form>
    <div>
        {{error}}
        <p>
            {{finalizo}}
        </p>
    </div>
    </div>

</section>